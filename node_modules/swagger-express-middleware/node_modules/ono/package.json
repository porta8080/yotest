    "karma-phantomjs-launcher": "^0.2.1",
    "karma-safari-launcher": "^0.1.1",
    "karma-sauce-launcher": "^0.2.14",
    "istanbul": "^0.3.19",
    "coveralls": "^2.11.4",
    "mocha": "^2.3.0",
    "chai": "^3.2.0",
    "sinon": "^1.16.1",
    "jscs": "^2.1.1",
    "jshint": "^2.8.0",
    "npm-check-updates": "^2.2.0",
    "version-bump-prompt": "^1.5.1",
    "karma-sinon": "^1.0.4"
  },
